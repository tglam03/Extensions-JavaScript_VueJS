{
  "css.lint.unknownAtRules": "ignore",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.glyphMargin": true,
  "editor.folding": false,
  "editor.minimap.enabled": true,
  "editor.lineNumbers": "on",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.mouseWheelScrollSensitivity": 1,
  "editor.fastScrollSensitivity": 1,
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "solid",
  "editor.stickyScroll.enabled": true,
  "editor.fontFamily": "'CaskaydiaMono Nerd Font Mono'",
  "editor.fontLigatures": true,
  "editor.lightbulb.enabled": "off",
  "breadcrumbs.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.lineHeight": 2,
  "editor.fontWeight": "normal",
  "editor.guides.indentation": true,
  "editor.guides.highlightActiveIndentation": true,
  "editor.wordWrapColumn": 50,
  "editor.wordWrap": "on",
  "editor.renderControlCharacters": true,
  "editor.accessibilitySupport": "off",
  "workbench.colorTheme": "Tokyo Night",
  "workbench.activityBar.location": "default",
  "workbench.hover.delay": 500,
  "workbench.sash.hoverDelay": 500,
  "workbench.editor.empty.hint": "hidden",
  "workbench.statusBar.visible": true,
  "workbench.startupEditor": "none",
  "workbench.preferredDarkColorTheme": "Tokyo Night",
  "workbench.layoutControl.enabled": true,
  "editor.scrollbar.scrollByPage": true,
  "zenMode.showTabs": "single",
  "zenMode.centerLayout": true,
  "zenMode.fullScreen": true,
  "editor.renderWhitespace": "all",
  "editor.showFoldingControls": "always",
  "editor.hover.enabled": false,
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.validation.template": false,
  "vetur.format.enable": false
}
